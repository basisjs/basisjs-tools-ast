{
  "name": "basisjs-tools-ast",
  "version": "1.6.1",
  "description": "Tool set for basisjs-tools to work with various types of AST",
  "homepage": "https://github.com/basisjs/basisjs-tools-ast",
  "author": "Roman Dvornov <rdvornov@gmail.com>",
  "license": "MIT",
  "repository": "basisjs/basisjs-tools-ast",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "rules": {
      "no-undef": 2,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "after-used"
        }
      ]
    }
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "htmlparser2": "~3.9.2",
    "csso": "~3.5.0",
    "escodegen": "^1.8.1",
    "esprima": "^3.1.3",
    "estraverse": "^4.2.0"
  },
  "devDependencies": {
    "mocha": "^2.3.3"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ]
}
